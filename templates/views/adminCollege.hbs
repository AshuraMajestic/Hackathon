<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="css/admin.css" />
    <link rel="stylesheet" href="../style/bootstrap.min.css" />
    <script src="https://use.fontawesome.com/3903c9d7fd.js"></script>
    <link
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.2/css/all.css"
      rel="stylesheet"
    />
    <title>HomePage</title>
  </head>
  <body class="row">
    <div class="col-1">
      {{>navbar}}
    </div>
    <div class="col-11 container center">
      <form class="row" action="/adminCollege" method="POST">
        <div class="col-12">
          <h1 class="heading">Enter College Detail</h1>
        </div>
        <div class="col-6">
          <input
            autocomplete="off"
            type="text0"
            class="form-control"
            id="inputname"
            name="collegename"
            required
          />
          <label for="inputname" class="form-label">College Name</label>
        </div>

        <div class="col-6">
          <input
            autocomplete="off"
            type="text"
            name="collegecode"
            class="form-control"
            id="inputCode"
            required
          />
          <label for="inputCode" class="form-label">College Code</label>
        </div>
        <div class="col-6">
          <input
            autocomplete="off"
            type="email"
            name="email"
            class="form-control"
            id="inputUser"
          />
          <label for="inputUser" class="form-label">Email</label>
        </div>
        <div class="col-6">
          <input
            autocomplete="off"
            type="text"
            name="universitycode"
            class="form-control"
            id="inputunicode"
          />
          <label for="inputunicode" class="form-label">University Code</label>
        </div>

        <div class="col-12 name">
          <input
            name="address"
            type="text"
            class="form-control"
            id="inputCollegeAdd"
            autocomplete="off"
          />
          <label for="inputCollegeCode" class="form-label">College Address</label>
        </div>

        <div class="col-12 center">
          <button type="submit" class="btn registerbtn">Register</button>
        </div>
      </form>
    </div>
    <script src="../js/bootstrap.js" type="text/javascript"></script>
  </body>
</html>