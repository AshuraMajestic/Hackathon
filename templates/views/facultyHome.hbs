<!DOCTYPE html>
<html lang="en">

<head>
    {{>headerFaculty}}
    <title>Home</title>
    <link rel="stylesheet" href="css/facultyProfile.css">
</head>

<body class="row">
    <div class="col-1">
        {{>navFaculty}}
    </div>
    <div class="col-11 "> 
     <form>
        <div class="container">
            <div class="title">Faculty details</div>
            <div class="content">
              <form action="#">
                <div class="user-details">
                  <div class="input-box">
                    <span class="details">Full Name</span>
                    <input type="text" placeholder="Yadav amit " required>
                  </div>
                  <div class="input-box">
                    <span class="details">Faculty ID</span>
                    <input type="text" placeholder="1224332843" required>
                  </div>
                  <div class="input-box">
                    <span class="details">Email</span>
                    <input type="text" placeholder="yadav@gmail.com" required>
                  <div class="input-box">
                    <span class="details">Expertise</span>
                    <input type="text" placeholder="web devloper" required>
                  </div>
                  <div class="input-box">
                    <span class="details">Experience</span>
                    <input type="text" placeholder="2 year" required>
                  </div>
                  <div class="input-box">
                    <span class="details">College code</span>
                    <input type="text" placeholder="103" required>
                  </div>
                </div>
                <div class="gender-details">
                  <input type="radio" name="gender" id="dot-1">
                  <input type="radio" name="gender" id="dot-2">
                  <input type="radio" name="gender" id="dot-3">
                  <span class="gender-title">Gender</span>
                  <div class="category">
                    <label for="dot-1">
                    <span class="gender">Male</span>
                  </label>
                </div>
                </div>
               
              </form>
            </div>
          </div>
     </form>
    </div>
     <script src="../js/bootstrap.js" type="text/javascript"></script>
</body>

</html>